{
  "name": "llm-report",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "prebuild": "prisma generate",
    "predev": "prisma generate",
    "stripe:webhook": "stripe listen --forward-to localhost:3001/api/v1/stripe/webhook",
    "stripe:fixtures": "stripe fixtures fixtures/stripe.json",
    "migrate:prod": "dotenv -e .env -- npx prisma db push",
    "migrate:dev": "dotenv -e .env.local -- npx prisma db push",
    "db:drop:dev": "dotenv -e .env.local -- npx prisma migrate reset",
    "db:drop:prod": "dotenv -e .env -- npx prisma migrate reset",
    "db:reset:dev": "yarn db:drop:dev && yarn migrate:dev",
    "db:reset:prod": "yarn db:drop:prod && yarn migrate:prod"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.13.0",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@stripe/stripe-js": "^1.52.1",
    "@tremor/react": "^2.4.1",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.6",
    "class-variance-authority": "^0.5.3",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "framer-motion": "^10.12.4",
    "loader-utils": "^3.2.1",
    "lucide-react": "^0.181.0",
    "next": "13.3.1",
    "next-auth": "^4.22.1",
    "papaparse": "^5.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-table": "^7.8.0",
    "stripe": "^12.3.0",
    "swr": "^2.1.5",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/react-table": "^7.7.14",
    "autoprefixer": "^10.4.14",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.23",
    "prisma": "^4.13.0"
  }
}
